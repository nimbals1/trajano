<?xml version="1.0" encoding="utf-8" ?>
<project xmlns="http://maven.apache.org/DECORATION/1.1.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/DECORATION/1.1.0 http://maven.apache.org/xsd/decoration-1.1.0.xsd">
	<skin>
		<groupId>org.apache.maven.skins</groupId>
		<artifactId>maven-fluido-skin</artifactId>
		<version>1.3.0</version>
	</skin>
	<bannerLeft>
		<src>images/trajano.png</src>
		<href>http://www.trajano.net</href>
		<alt>Trajano</alt>
	</bannerLeft>
	<poweredBy>
		<logo img="images/powered-by-trajano.png" href="http://www.trajano.net"
			width="90" height="30" alt="Trajano" name="Trajano" />
	</poweredBy>
	<googleAnalyticsAccountId>UA-32387215-1</googleAnalyticsAccountId>
	<body>
		<menu ref="reports" inherit="top" />
	</body>
	<custom>
		<fluidoSkin>
			<gitHub>
				<projectId>trajano/${project.artifactId}</projectId>
				<ribbonOrientation>right</ribbonOrientation>
				<ribbonColor>black</ribbonColor>
			</gitHub>
			<facebookLike />
			<googlePlusOne />
		</fluidoSkin>
	</custom>
</project>
